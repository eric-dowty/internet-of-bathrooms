<div id="orange-background">
</div>

<div id="gray-background">
</div>

<div id="github-logout">
  <%= button_to "Logout", logout_path, class: 'github-btn', id: 'logout-btn', method: :delete %>
</div>

<div id="iob-main">
  <%= image_tag("iob.png") %>
</div>

<div id="avatar">
  <%= image_tag(current_user.image_url) %>
</div>

<div id="bathroom-subtitle">
  <p>Big workspace bathroom availability.</p>
</div>

<%= render partial: 'bathrooms/score' %>

<%= render partial: 'bathrooms/trivia' %>

<%= render partial: 'bathrooms/answer' %>

<%= render partial: 'bathrooms/status', :locals => { :toilet_status => @toilet_status } %>


<%= image_tag("bathroom-bot-2.png", id: "bathroom-bot") %>
<div id="bathroom-bot-about">
  <button type="button" id="bathroom-bot-about-btn">?</button>
</div>
<div id="bathroom-notifier">
  <i><span id="bathroom-bot-question"></span></i>
</div>

<script type="text/javascript">toiletStatus()</script>
<script type="text/javascript">triviaForm()</script>
<script type="text/javascript">clearPlaceholder()</script>
<script type="text/javascript">triviaSubmit()</script>
<script type="text/javascript">skipQuestion()</script>
<script type="text/javascript">botInfo()</script>
